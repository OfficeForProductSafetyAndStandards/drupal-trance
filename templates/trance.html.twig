{#
/**
 * @file trance.html.twig
 * Default theme implementation to present cms_content data.
 *
 * This template is used when viewing cms_content pages.
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_trance()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('trance') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
